{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Ping",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "https://upload.wikimedia.org/*"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "https://upload.wikimedia.org/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}