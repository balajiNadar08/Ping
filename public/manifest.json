{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Ping",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "https://upload.wikimedia.org/*",
    "storage" 
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "https://upload.wikimedia.org/*"
  ],
  "icons": {
    "32": "icon32.png"
  }
}